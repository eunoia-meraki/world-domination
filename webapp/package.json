{
  "name": "aw_frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod:build": "pnpm clean && pnpm relay && pnpm build",
    "clean": "rm -f -r build",
    "start": "pnpm clean && cross-env BROWSER=none craco start",
    "build": "craco build",
    "relay": "relay-compiler",
    "update-schema": "get-graphql-schema http://localhost:8002/graphql > schema/schema.graphql",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier": "prettier --write \"**/*.+(ts|tsx)\"",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles --config svgo-config.json $FILE",
    "svgo-all": "cross-env svgo -r -f src/components/icons/svg --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles --config svgo-config.json"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema/schema.graphql",
    "customScalars": {
      "DateTime": "string",
      "UInt": "number",
      "UShort": "number"
    },
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**"
    ],
    "language": "typescript"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@craco/craco": "^7.0.0-alpha.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.69",
    "@mui/material": "^5.4.1",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.4.2",
    "babel-plugin-relay": "^13.1.1",
    "browserslist": "^4.19.1",
    "freeice": "^2.2.2",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "meros": "1.2.0-next.4",
    "postcss": "^8.4.6",
    "prop-types": "^15.8.1",
    "react-data-grid": "^7.0.0-beta.11",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-flow-renderer": "^9.7.4",
    "react-grid-layout": "^1.3.3",
    "react-hotkeys": "^2.0.0",
    "react-location": "^3.3.4",
    "react-location-devtools": "^3.3.4",
    "react-relay": "^13.1.1",
    "react-scripts": "^5.0.0",
    "recharts": "^2.1.9",
    "relay-runtime": "^13.1.1",
    "subscriptions-transport-ws": "^0.11.0",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@ch1ffa/relay-compiler-webpack-plugin": "^0.1.6",
    "@types/babel__core": "^7.1.18",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.17",
    "@types/react-dom": "^17.0.11",
    "@types/react-relay": "^13.0.1",
    "@types/recharts": "^1.8.23",
    "@types/relay-compiler": "^8.0.2",
    "@types/relay-runtime": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "get-graphql-schema": "^2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "7.0.4",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "relay-compiler": "^13.1.1",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1"
  }
}
